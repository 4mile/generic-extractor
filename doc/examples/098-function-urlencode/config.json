{
    "parameters": {
        "api": {
            "baseUrl": "http://mock-server:80/098-function-urlencode/"
        },
        "config": {
            "debug": true,
            "outputBucket": "mock-server",
            "jobs": [
                {
                    "endpoint": "users",
                    "dataType": "users",
                    "children": [
                        {
                            "endpoint": "company/{companyName}",
                            "dataType": "company",
                            "method": "GET",
                            "dataField": ".",
                            "placeholders": {
                                "companyName": {
                                    "path": "company",
                                    "function": "urlencode",
                                    "args": [
                                        {
                                            "placeholder": "value"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                }
            ]
        }
    }
}
